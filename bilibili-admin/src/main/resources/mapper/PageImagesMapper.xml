<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhn.bilibili.admin.mapper.PageImagesMapper">

    <resultMap id="BaseResultMap" type="com.xhn.bilibili.admin.entity.PageImagesEntity">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="imageUrl" column="image_url" jdbcType="VARCHAR"/>
            <result property="describe" column="describe" jdbcType="VARCHAR"/>
            <result property="type" column="type" jdbcType="INTEGER"/>
            <result property="showStatus" column="show_status" jdbcType="INTEGER"/>
            <result property="status" column="status" jdbcType="INTEGER"/>
            <result property="updateId" column="update_id" jdbcType="BIGINT"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,image_url,describe,
        type,show_status,status,
        update_id,update_time,create_time
    </sql>
    <select id="getPageImageList" resultType="com.xhn.bilibili.admin.entity.PageImagesEntity">
        select * from page_images
    </select>

</mapper>
